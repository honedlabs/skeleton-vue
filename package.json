{
  "name": "@honed/:package_name",
  "version": "0.0.1",
  "description": ":package_description",
  "keywords": [
    "honed",
    "vue",
    "inertia",
    ":package_name"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run format && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "format": "npx @biomejs/biome format --write src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm publish",
    "init": "npx node ./init.js && npm i",
    "visualise": "npx vite-bundle-visualizer -o ./tmp/bundle-visualizer.html"
  },
  "author": ":package_author",
  "repository": {
    "type": "git",
    "url": "https://github.com/honedlabs/:package_name.git"
  },
  "homepage": "https://github.com/honedlabs/:package_name#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/:package_name.umd.cjs",
  "module": "./dist/:package_name.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/:package_name.js",
      "require": "./dist/:package_name.umd.cjs"
    }
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "@inertiajs/vue3": "^1.0.0||^2.0.0",
    "axios": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.3",
    "axios": "^1.2.0",
    "typescript": "^5.7.0",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
